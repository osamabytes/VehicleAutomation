{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7010"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/inventory/{**path}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7018
        }
      ],
      "UpstreamPathTemplate": "/api/inventory/{**path}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "PATCH" ]
    },
    {
      "DownstreamPathTemplate": "/api/orders/{**path}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7248
        }
      ],
      "UpstreamPathTemplate": "/api/order/{**path}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "PATCH" ]
    }
  ]
}
